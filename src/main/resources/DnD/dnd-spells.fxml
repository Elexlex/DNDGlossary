<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="700.0" prefWidth="910.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DnD.SpellWindowController">
   <children>
      <ImageView fitHeight="705.0" fitWidth="910.0" pickOnBounds="true">
         <image>
            <Image url="@CSS/Backgrounds/backgroundPaper.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="spellTableView" fixedCellSize="30.0" layoutX="169.0" layoutY="138.0" onMouseClicked="#doubleMouseClicked" pickOnBounds="false" prefHeight="535.0" prefWidth="700.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
      <Pane prefHeight="705.0" prefWidth="130.0">
         <children>
            <Button id="1" layoutX="20.0" layoutY="55.0" mnemonicParsing="false" prefHeight="90.0" prefWidth="90.0" />
            <Button layoutX="20.0" layoutY="180.0" mnemonicParsing="false" prefHeight="90.0" prefWidth="90.0" />
            <Button layoutX="20.0" layoutY="305.0" mnemonicParsing="false" prefHeight="90.0" prefWidth="90.0" />
            <Button layoutX="20.0" layoutY="430.0" mnemonicParsing="false" prefHeight="90.0" prefWidth="90.0" />
            <Button layoutX="20.0" layoutY="555.0" mnemonicParsing="false" prefHeight="90.0" prefWidth="90.0" />
         </children>
      </Pane>
      <ComboBox fx:id="schoolBox" layoutX="510.0" layoutY="49.0" prefWidth="150.0" promptText="Spell School" />
      <ComboBox fx:id="levelBox" layoutX="686.0" layoutY="49.0" prefWidth="150.0" promptText="Spell Level" />
      <TextField fx:id="searchName" layoutX="169.0" layoutY="49.0" prefHeight="25.0" prefWidth="235.0" promptText="Spell Name" />
   </children>
</AnchorPane>
